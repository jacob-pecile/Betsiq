<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.betsiq_server.betsiq">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/betsiq_logo"
        android:label="Betsiq"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login"></activity>
        <activity
            android:name=".SignUp"
            android:label="Trendy Tunes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabActivity"
            android:label="@string/title_activity_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".topHundred"></activity>
        <activity android:name=".myList"></activity>
        <activity android:name=".Groups"></activity>
        <activity android:name=".earnMoney"></activity>
        <activity android:name=".Settings"></activity>
        <activity android:name=".Group"></activity>
    </application>

</manifest>